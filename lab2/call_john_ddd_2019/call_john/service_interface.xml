<?xml version="1.0" encoding="utf-8"?>
<service_interface>
  <action name="MakeCall">
    <parameters>
      <parameter predicate="selected_contact" format="value"/>
      <parameter predicate="selected_number" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="CallJohnDevice"/>
    </target>
  </action>

  <query name="phone_number_of_contact">
      <parameters>
          <parameter predicate="selected_contact" format="value"/>
          <parameter predicate="selected_number" format="value"/>
        </parameters>
        
      <target>
        <device_module device="CallJohnDevice"/>
      </target>
    </query> 

  <validator name="PhoneNumberAvailable">
      <parameters>
        <parameter predicate="selected_contact" format="value"/>
      </parameters>
      <target>
        <device_module device="CallJohnDevice"/>
      </target>
    </validator>
    

</service_interface>